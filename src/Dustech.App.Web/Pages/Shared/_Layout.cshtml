@using Microsoft.AspNetCore.Mvc.TagHelpers
@model LayoutModel
<!DOCTYPE html>
<html lang="en" data-bs-theme="dark">
<partial name="_Head" model="@Model.Title"/>
<body>
<partial name="_Header" model="@Model.RequestCultureFeature"/>

@await RenderSectionAsync("BeforeMain", required: false)

<main class="main" role="main">
    @RenderBody()
</main>


<partial name="_Footer"/>

<script src="~/js/bootstrap.bundle.min.js"></script>
<script src="~/js/brand-bar-events.js" asp-append-version="true"></script>
<script src="~/js/site.js" asp-append-version="true"></script>

@await RenderSectionAsync("Scripts", required: false)
</body>
</html>